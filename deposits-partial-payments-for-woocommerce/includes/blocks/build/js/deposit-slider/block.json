{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "awcdp/deposit-slider",
  "version": "1.0.0",
  "title": "AWCDP Deposit Slider",
  "description": "Adds a deposit/full payment toggle to the checkout page.",
  "category": "awcdp-blocks",
  "supports": {
    "html": false,
    "align": false,
    "multiple": false,
    "reusable": false
  },
  "parent": [
    "woocommerce/checkout-totals-block"
  ],
  "attributes": {
    "lock": {
      "type": "object",
      "default": {
        "remove": true,
        "move": false
      }
    }
  },
  "textdomain": "deposits-partial-payments-for-woocommerce"
}
